{
    "manifest_version": 3,
    "name": "Digital Notice Board",
    "description": "A digital notice board with widgets for Google Slides and a Pomodoro timer.",
    "version": "1.0",
    "permissions": ["activeTab"],
    "chrome_url_overrides": {
      "newtab": "index.html"
    },
    "action": {
      "default_popup": "popup.html"
    },
    "icons": {
      "48": "icon.png",
      "128": "icon128.png"
    }
  }
  